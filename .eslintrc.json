{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true,
		"node": true
	},
	"extends": ["airbnb-base"],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"linebreak-style": ["error", "windows"],
		"no-console": "off",
		"no-tabs": "off",
		"indent": ["error", "tab"],
		"implicit-arrow-linebreak": "off",
		"comma-dangle": "off",
		"function-paren-newline": "off"
	}
}
